#!/bin/sh
#
# lcd        Starts lcd.
#

start() {
	echo -e "\033[9;0]" > /dev/tty0
}
stop() {
	echo "Stopping screen..."
}
restart() {
	stop
	start
}

case "$1" in
  start)
	start
	;;
  stop)
	stop
	;;
  restart|reload)
	restart
	;;
  *)
	echo "Usage: $0 {start|stop|restart}"
	exit 1
esac

exit $?

